    <validationRules>
        <fullName>ACCL__Prevent_Product_Assortment_Template</fullName>
        <active>true</active>
        <description>The Product Assorment Template can&apos;t be edited after Product Assortment creation.</description>
        <errorConditionFormula>ISCHANGED(ACCL__Product_Assortment_Template__c  )</errorConditionFormula>
        <errorMessage>The Product Assorment Template can&apos;t be edited after Product Assortment creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Valid_From_Valid_Thru_Validation</fullName>
        <active>true</active>
        <description>Valid From can&apos;t be later than Valid Thru.</description>
        <errorConditionFormula>ACCL__Valid_From__c  &gt;  ACCL__Valid_Thru__c</errorConditionFormula>
        <errorMessage>Valid From can&apos;t be later than Valid Thru.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Valid_Version_Off_Platform_Of_Version</fullName>
        <active>true</active>
        <description>Validates new Version Off Platform should not be more than the existing Version.</description>
        <errorConditionFormula>ACCL__Version__c &lt; ACCL__Version_Off_Platform_APL__c</errorConditionFormula>
        <errorMessage>Updating Version Off Platform should not be more than existing Version.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Validate_Version_Off_Platform_APL</fullName>
        <active>true</active>
        <description>Validates new Version Off Platform should not be less than the existing Version Off Platform.</description>
        <errorConditionFormula>OR(ACCL__Version_Off_Platform_APL__c &lt; PRIORVALUE(ACCL__Version_Off_Platform_APL__c),
          AND(ISBLANK(ACCL__Version_Off_Platform_APL__c), NOT(ISBLANK(PRIORVALUE(ACCL__Version_Off_Platform_APL__c)))))</errorConditionFormula>
        <errorMessage>Updating Version Off Platform should not be prior to the existing Version Off Platform.</errorMessage>
    </validationRules>

    <validationRules>
        <fullName>ACCL__Active_And_Mark_for_Deletion</fullName>
        <active>true</active>
        <description>Mark for Deletion and Active can&apos;t be checked at the same time.</description>
        <errorConditionFormula>AND(ACCL__Active__c,ACCL__Mark_for_Deletion__c)</errorConditionFormula>
        <errorMessage>Mark for Deletion and Active can&apos;t be checked at the same time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Activity_Score_must_be_greater_than_zero</fullName>
        <active>true</active>
        <description>If Consider Score is yes then Activity Score must be greater than zero.</description>
        <errorConditionFormula>AND(ISPICKVAL(ACCL__Consider_Score__c, &quot;Yes&quot;) ,   OR(ACCL__Activity_Score__c &lt;= 0, ISBLANK(ACCL__Activity_Score__c)))</errorConditionFormula>
        <errorMessage>Activity Score must be greater than zero for a score relevant Activity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Date_From_Has_To_Be_Before_Date_To</fullName>
        <active>true</active>
        <errorConditionFormula>ACCL__Valid_From__c &gt; ACCL__Valid_Thru__c</errorConditionFormula>
        <errorMessage>Valid From can&apos;t be later than Valid Thru.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__JDL_Active_FSA_Phase</fullName>
        <active>true</active>
        <description>Activity can&apos;t be activated if the Field Sales Activity Phase is &apos;Planning&apos; or &apos;Cancelled&apos;.</description>
        <errorConditionFormula>AND(      
OR(
ISPICKVAL(ACCL__Field_Sales_Activity__r.ACCL__Phase__c, &quot;Planning&quot;),
ISPICKVAL(ACCL__Field_Sales_Activity__r.ACCL__Phase__c, &quot;Canceled&quot;)),
ACCL__Active__c
)</errorConditionFormula>
        <errorMessage>Activity can&apos;t be activated if the Field Sales Activity Phase is &apos;Planning&apos; or &apos;Cancelled&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__JDL_Time_Frame_FSA_Time_Frame</fullName>
        <active>true</active>
        <description>The Activity time frame must be within the Field Sales Activity time frame.</description>
        <errorConditionFormula>NOT(ISBLANK(ACCL__Field_Sales_Activity__c)) &amp;&amp; (ACCL__Valid_From__c &lt; ACCL__Field_Sales_Activity__r.ACCL__Valid_From__c || ACCL__Valid_Thru__c &gt; ACCL__Field_Sales_Activity__r.ACCL__Valid_Thru__c)</errorConditionFormula>
        <errorMessage>The Activity time frame must be within the Field Sales Activity time frame.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Only_Sellable_Promotions_Allowed</fullName>
        <active>true</active>
        <description>Only sellable promotions can be assigned to activity.</description>
        <errorConditionFormula>AND(ACCL__Promotion__r.Id != NULL, ACCL__Promotion__r.RecordType.DeveloperName != &apos;SellablePromotion&apos;)</errorConditionFormula>
        <errorMessage>Only promotions with &apos;Sellable&apos; Promotion Template can be selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Prevent_Changing_of_JDLTemplate_referenc</fullName>
        <active>true</active>
        <errorConditionFormula>NOT ISNEW()  &amp;&amp;  ISCHANGED(ACCL__Job_Definition_List_Template__c)</errorConditionFormula>
        <errorDisplayField>ACCL__Job_Definition_List_Template__c</errorDisplayField>
        <errorMessage>The Activity Template can&apos;t be edited after Activity creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Restrict_Editability_After_Activation</fullName>
        <active>true</active>
        <description>Call Template, Default Management Type, User Policy, Valid From and Valid Thru can&apos;t be edited if the Active is checked.</description>
        <errorConditionFormula>AND
(
	NOT(ISNEW()),
	ACCL__Active__c,
	OR
	(
		ISCHANGED(ACCL__Call_Template__c), 
		ISCHANGED(ACCL__Default_Management_Type__c),
		ISCHANGED(ACCL__User_Policy__c),
		(ISCHANGED(ACCL__Valid_From__c) &amp;&amp; PRIORVALUE(ACCL__Valid_From__c) &lt; TODAY()),
		(ISCHANGED(ACCL__Valid_Thru__c) &amp;&amp; PRIORVALUE(ACCL__Valid_Thru__c) &lt; TODAY())
	) 
)</errorConditionFormula>
        <errorMessage>Call Template, Default Management Type, User Policy, Valid From and Valid Thru can&apos;t be edited if the Active is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_Org_should_be_same_as_template</fullName>
        <active>true</active>
        <description>The Activity and Activity Template must have the same Sales Org.</description>
        <errorConditionFormula>TEXT( ACCL__Sales_Org__c ) != TEXT( ACCL__Job_Definition_List_Template__r.ACCL__Sales_Org__c)</errorConditionFormula>
        <errorMessage>The Activity and Activity Template must have the same Sales Org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_Org_should_be_same_for_all_lookups</fullName>
        <active>true</active>
        <description>The Call Template and Job Definition List must have the same Sales Org.</description>
        <errorConditionFormula>AND (ACCL__Field_Sales_Activity__r.Id = null, (TEXT( ACCL__Call_Template__r.ACCL__Sales_Org__c ) &lt;&gt; TEXT( ACCL__Sales_Org__c )))</errorConditionFormula>
        <errorMessage>The Call Template and Activity must have the same Sales Org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_Org_should_be_same_for_lookup_fsa</fullName>
        <active>true</active>
        <description>The Call Template, Job Definition List and Field Sales Activity must have the same Sales Org.</description>
        <errorConditionFormula>AND( ACCL__Field_Sales_Activity__r.Id  &lt;&gt; null , (
          TEXT( ACCL__Field_Sales_Activity__r.ACCL__Sales_Org__c )  &lt;&gt;
          TEXT( ACCL__Call_Template__r.ACCL__Sales_Org__c )
          ||
          TEXT( ACCL__Field_Sales_Activity__r.ACCL__Sales_Org__c ) &lt;&gt;
          TEXT( ACCL__Sales_Org__c )))</errorConditionFormula>
        <errorMessage>The Call Template, Activity and Field Sales Activity must have the same Sales Org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_Org_should_be_same_for_promotion</fullName>
        <active>true</active>
        <description>The Promotion and the Activity must have the same Sales Org.</description>
        <errorConditionFormula>AND( ACCL__Promotion__r.Id != null, ACCL__Promotion__r.ACCL__Sales_Org__c != TEXT( ACCL__Sales_Org__c ))</errorConditionFormula>
        <errorMessage>The Promotion and the Activity must have the same Sales Org.</errorMessage>
    </validationRules>

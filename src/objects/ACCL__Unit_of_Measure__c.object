    </validationRules>
    <validationRules>
        <fullName>ACCL__Rounding_Rule_Validation</fullName>
        <active>true</active>
        <description>Round Rule must be &apos;Never&apos; if the Unit of Measure has the highest Sort value.</description>
        <errorConditionFormula>(ACCL__Product__r.ACCL__Rounding_Rule_Validation__c &lt;=  ACCL__Sort__c  ||  ISBLANK( ACCL__Product__r.ACCL__Rounding_Rule_Validation__c )  )  &amp;&amp;  NOT(ISPICKVAL(ACCL__Rounding_Rule__c, &apos;Never&apos;))</errorConditionFormula>
        <errorMessage>Round Rule must be &apos;Never&apos; if the Unit of Measure has the highest Sort value.</errorMessage>
    </validationRules>

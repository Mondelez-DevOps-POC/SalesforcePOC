    <validationRules>
        <fullName>ACCL__AdditionalKPIs_CompatibleToProductLevel</fullName>
        <active>true</active>
        <description>The BOM Scope of Additional KPI Definition must contain ‘Component’ if Product Level is ‘Component’.</description>
        <errorConditionFormula>AND(
    ISPICKVAL(ACCL__Product_Level__c, &apos;Component&apos;),
    OR(
        AND(
            NOT(INCLUDES(ACCL__Additional_KPI_Definition_1__r.ACCL__BoM_Scope__c, &apos;Component&apos;)),
            NOT(ISBLANK(ACCL__Additional_KPI_Definition_1__c))
        ),
        AND(
            NOT(INCLUDES(ACCL__Additional_KPI_Definition_2__r.ACCL__BoM_Scope__c, &apos;Component&apos;)),
            NOT(ISBLANK(ACCL__Additional_KPI_Definition_2__c))
        ),
        AND(
            NOT(INCLUDES(ACCL__Additional_KPI_Definition_3__r.ACCL__BoM_Scope__c, &apos;Component&apos;)),
            NOT(ISBLANK(ACCL__Additional_KPI_Definition_3__c))
        )
    )
)</errorConditionFormula>
        <errorDisplayField>ACCL__Product_Level__c</errorDisplayField>
        <errorMessage>The BOM Scope of Additional KPI Definitions must contain ‘Component’ if Product Level is ‘Component’.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__BOMScope_AllowProdLvlComponentValidation</fullName>
        <active>true</active>
        <description>The BOM Scope of Source KPI Definition must contain ‘Component’ if Allowed Product Level is ‘Component’.</description>
        <errorConditionFormula>AND(
INCLUDES( ACCL__Allowed_Product_Levels__c ,&apos;Component&apos;),
NOT(INCLUDES(ACCL__Source_KPI_Definition__r.ACCL__BoM_Scope__c, &apos;Component&apos;))
)</errorConditionFormula>
        <errorDisplayField>ACCL__Allowed_Product_Levels__c</errorDisplayField>
        <errorMessage>The BOM Scope of Source KPI Definition must contain ‘Component’ if Allowed Product Level is ‘Component’.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__BOMScope_AllowProdLvlProductValidation</fullName>
        <active>true</active>
        <description>The BOM Scope of Source KPI Definition must not contain ‘Component’ if Allowed Product Level is ‘Product’.</description>
        <errorConditionFormula>AND(
INCLUDES( ACCL__Allowed_Product_Levels__c ,&apos;Product&apos;),
INCLUDES(ACCL__Source_KPI_Definition__r.ACCL__BoM_Scope__c, &apos;Component&apos;))</errorConditionFormula>
        <errorDisplayField>ACCL__Allowed_Product_Levels__c</errorDisplayField>
        <errorMessage>The BOM Scope of Source KPI Definition must not contain ‘Component’ if Allowed Product Level is ‘Product’.</errorMessage>
    </validationRules>

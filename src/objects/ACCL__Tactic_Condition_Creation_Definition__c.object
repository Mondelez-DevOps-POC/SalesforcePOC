    <validationRules>
        <fullName>ACCL__ProductLevel_InAvailableProductLevel</fullName>
        <active>true</active>
        <description>The Product Level must be included in the Available Product Level of the promotion template.</description>
        <errorConditionFormula>OR(
	AND(
		ISPICKVAL(ACCL__Product_Level__c, &apos;Category&apos;),
		NOT(INCLUDES(ACCL__Tactic__r.ACCL__Promotion__r.ACCL__Promotion_Template__r.ACCL__Available_Product_Level__c,  &apos;Category&apos; ))
	),
	AND(
		ISPICKVAL(ACCL__Product_Level__c, &apos;SubCategory&apos;),
		NOT(INCLUDES(ACCL__Tactic__r.ACCL__Promotion__r.ACCL__Promotion_Template__r.ACCL__Available_Product_Level__c,  &apos;SubCategory&apos; ))
	),
	AND(
		ISPICKVAL(ACCL__Product_Level__c, &apos;Brand&apos;),
		NOT(INCLUDES(ACCL__Tactic__r.ACCL__Promotion__r.ACCL__Promotion_Template__r.ACCL__Available_Product_Level__c,  &apos;Brand&apos; ))
	),
	AND(
		ISPICKVAL(ACCL__Product_Level__c, &apos;Flavor&apos;),
		NOT(INCLUDES(ACCL__Tactic__r.ACCL__Promotion__r.ACCL__Promotion_Template__r.ACCL__Available_Product_Level__c,  &apos;Flavor&apos; ))
	),
	AND(
		ISPICKVAL(ACCL__Product_Level__c, &apos;Package&apos;),
		NOT(INCLUDES(ACCL__Tactic__r.ACCL__Promotion__r.ACCL__Promotion_Template__r.ACCL__Available_Product_Level__c,  &apos;Package&apos; ))
	)
)</errorConditionFormula>
        <errorDisplayField>ACCL__Product_Level__c</errorDisplayField>
        <errorMessage>The Product Level must be included in the Available Product Level of the promotion template.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__SourceKPI_EditableWithTotal</fullName>
        <active>true</active>
        <description>The Source KPI Definition must have Record Type &apos;Editable&apos; and Edit Mode &apos;Total&apos;.</description>
        <errorConditionFormula>OR(
    ACCL__Source_KPI_Definition__r.RecordType.DeveloperName &lt;&gt; &apos;editable&apos;,
    NOT(ISPICKVAL(ACCL__Source_KPI_Definition__r.ACCL__Editable_Edit_Mode__c, &apos;total&apos;))
)</errorConditionFormula>
        <errorDisplayField>ACCL__Source_KPI_Definition__c</errorDisplayField>
        <errorMessage>The Source KPI Definition must have Record Type &apos;Editable&apos; and Edit Mode &apos;Total&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__SourceKPI_NotEmpty</fullName>

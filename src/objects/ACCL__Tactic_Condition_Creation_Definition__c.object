        <errorConditionFormula>NOT(ISPICKVAL(ACCL__Source_KPI_Definition__r.ACCL__Scope__c, &apos;promotiontactic&apos;))</errorConditionFormula>
        <errorDisplayField>ACCL__Source_KPI_Definition__c</errorDisplayField>
        <errorMessage>The Source KPI Definition must have Object Scope ‘Promotion Tactic’.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__SourceKpi_CompatibleToProductLevel</fullName>
        <active>true</active>
        <description>The BOM Scope of Source KPI Definition must contain ‘Component’ if Product Level is ‘Component’.</description>
        <errorConditionFormula>AND(
    ISPICKVAL(ACCL__Product_Level__c, &apos;Component&apos;),

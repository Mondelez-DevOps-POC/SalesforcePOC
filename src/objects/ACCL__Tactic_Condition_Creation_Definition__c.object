)</errorConditionFormula>
        <errorDisplayField>ACCL__Source_KPI_Definition__c</errorDisplayField>
        <errorMessage>The BOM Scope of Source KPI Definition must contain ‘Component’ if Product Level is ‘Component’.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Targets_CheckMaintenanceBeforeUpdate</fullName>
        <active>true</active>
        <description>The Targets field can be changed after creation only if Maintenance field is set to &apos;Initial&apos;.</description>

ISPICKVAL(ACCL__Product_Level__c, &apos;Component&apos;),
NOT(INCLUDES(ACCL__Source_KPI_Definition__r.ACCL__BoM_Scope__c, &apos;Component&apos;))
)</errorConditionFormula>
        <errorDisplayField>ACCL__Product_Level__c</errorDisplayField>

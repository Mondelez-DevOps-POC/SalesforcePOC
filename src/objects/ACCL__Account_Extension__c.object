    <validationRules>
        <fullName>ACCL__Account_Plan_Type_Timeframe</fullName>
        <active>true</active>
        <description>Customer must have the Promotion Role assigned (Promotion Valid From and Promotion Valid Thru) if the Account Plan Type is &apos;Plan&apos;.</description>
        <errorConditionFormula>AND(ISPICKVAL(ACCL__Account_Plan_Type__c , &apos;Plan&apos;),
      OR(
      ISBLANK(ACCL__Promotion_Valid_From__c),

    <validationRules>
        <fullName>ACCL__Sales_Org_Mismatch</fullName>
        <active>true</active>
        <description>The Tactic and the Product must have the same Sales Org.</description>
        <errorConditionFormula>TEXT(ACCL__Tactic__r.ACCL__Tactic_Template__r.ACCL__Sales_Org__c) &lt;&gt;  ACCL__Product__r.ACCL__Sales_Org__c</errorConditionFormula>
        <errorMessage>The Tactic and the Product must have the same Sales Org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__product_level_should_be_product</fullName>
        <active>true</active>
        <description>Only products with &apos;Product&apos; Product Level can be selected.</description>
        <errorConditionFormula>AND(
ACCL__Tactic__r.ACCL__Promotion__r.ACCL__Promotion_Template__r.ACCL__Selectable_Promotion__c = true,
NOT (ISPICKVAL(ACCL__Product__r.ACCL__Product_Level__c , &quot;Product&quot;)))</errorConditionFormula>
        <errorMessage>Only products with &apos;Product&apos; Product Level can be selected.</errorMessage>
    </validationRules>

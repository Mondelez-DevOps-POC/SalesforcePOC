    <validationRules>
        <fullName>ACCL__Avoid_Sales_Org_Modification</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(ACCL__Sales_Org__c)</errorConditionFormula>
        <errorMessage>The Sales Org can&apos;t be edited after ContractTemplate creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Contract_Template_Rule</fullName>
        <active>true</active>
        <description>The Customer Contract Template can&apos;t be empty.</description>
        <errorConditionFormula>AND( RecordType.DeveloperName == &apos;Contract_Tactic_Template&apos;,  ISBLANK(ACCL__Account_Contract_Template__r.Id ))</errorConditionFormula>
        <errorDisplayField>ACCL__Account_Contract_Template__c</errorDisplayField>
        <errorMessage>The Customer Contract Template can&apos;t be empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Job_Definition_List_can_not_be_change</fullName>
        <active>false</active>
        <errorConditionFormula>ISCHANGED( ACCL__Job_Definition_List__c )</errorConditionFormula>
        <errorMessage>The Activity can&apos;t be edited after Contract Template creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Maximum_Days</fullName>
        <active>true</active>
        <errorConditionFormula>AND( RecordType.DeveloperName == &apos;Customer_Contract_Template&apos;,   OR( ISBLANK( ACCL__Max_Days__c  ) ,ACCL__Max_Days__c  &lt; 1, ACCL__Max_Days__c &lt;  ACCL__Min_Days__c))</errorConditionFormula>
        <errorDisplayField>ACCL__Max_Days__c</errorDisplayField>
        <errorMessage>The Max Days must be a number equal or greater than 1 and equal or greater than the Min Days.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Minimum_Days</fullName>
        <active>true</active>
        <errorConditionFormula>AND( RecordType.DeveloperName == &apos;Customer_Contract_Template&apos;,           OR( ISBLANK(ACCL__Min_Days__c ) ,ACCL__Min_Days__c &lt; 1))</errorConditionFormula>
        <errorDisplayField>ACCL__Min_Days__c</errorDisplayField>
        <errorMessage>The Min Days must be a number equal or greater than 1.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_Org_should_be_same_for_Workflow</fullName>
        <active>true</active>
        <description>The Contract Template and Workflow must have the same Sales Org.</description>
        <errorConditionFormula>NOT(ISBLANK( ACCL__Workflow__c )) &amp;&amp; TEXT(ACCL__Sales_Org__c) &lt;&gt; TEXT(ACCL__Workflow__r.ACCL__Sales_Org__c)</errorConditionFormula>
        <errorMessage>The Contract Template and Workflow must have the same Sales Org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_org</fullName>
        <active>true</active>
        <description>The Customer Contract Template and the Contract Template must have the same Sales Org.</description>
        <errorConditionFormula>AND( RecordType.DeveloperName == &apos;Contract_Tactic_Template&apos;,TEXT(ACCL__Account_Contract_Template__r.ACCL__Sales_Org__c) !=  TEXT(ACCL__Sales_Org__c))</errorConditionFormula>
        <errorDisplayField>ACCL__Sales_Org__c</errorDisplayField>
        <errorMessage>The Customer Contract Template and the Contract Template must have the same Sales Org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_org_cannot_be_changed</fullName>
        <active>false</active>
        <errorConditionFormula>ISCHANGED( ACCL__Sales_Org__c )</errorConditionFormula>
        <errorDisplayField>ACCL__Sales_Org__c</errorDisplayField>
        <errorMessage>The Sales Org can&apos;t be edited after Contract Template creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Workflow_Required_Field</fullName>
        <active>true</active>
        <description>The Workflow can&apos;t be empty.</description>
        <errorConditionFormula>AND((RecordType.DeveloperName == &apos;Customer_Contract_Template&apos;), ISBLANK(ACCL__Workflow__c))</errorConditionFormula>
        <errorDisplayField>ACCL__Workflow__c</errorDisplayField>
        <errorMessage>The Workflow can&apos;t be empty.</errorMessage>
    </validationRules>

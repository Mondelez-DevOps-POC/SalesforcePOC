    <validationRules>
        <fullName>ACCL__Check_If_Customer_Is_Empty</fullName>
        <active>true</active>
        <description>If Sales_Folder_Template has field Anchor_Type as BusinessPartner then Account__c cannot be empty.</description>
        <errorConditionFormula>ISPICKVAL(ACCL__Sales_Folder_Template__r.ACCL__Anchor_Type__c, &apos;BusinessPartner&apos;) &amp;&amp; ISBLANK(ACCL__Account__c)</errorConditionFormula>
        <errorMessage>The Customer cannot be empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Check_If_Customer_Sales_Org_Matches</fullName>
        <active>true</active>
        <description>The Customer and the Sales Folder must have the same Sales Org.</description>
        <errorConditionFormula>ISPICKVAL(ACCL__Sales_Folder_Template__r.ACCL__Anchor_Type__c, &apos;BusinessPartner&apos;) &amp;&amp; NOT(ISBLANK(ACCL__Account__c)) &amp;&amp; ACCL__Account__r.ACCL__Sales_Org__c &lt;&gt; TEXT(ACCL__Sales_Org__c)</errorConditionFormula>
        <errorMessage>The Customer and the Sales Folder must have the same Sales Org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Check_If_Org_Unit_Is_Empty</fullName>
        <active>true</active>
        <description>If Sales_Folder_Template has field Anchor_Type as OrgUnit then Org_Unit__c cannot be empty.</description>
        <errorConditionFormula>ISPICKVAL(ACCL__Sales_Folder_Template__r.ACCL__Anchor_Type__c, &apos;OrgUnit&apos;) &amp;&amp; ISBLANK(ACCL__Org_Unit__c)</errorConditionFormula>
        <errorMessage>The Org Unit cannot be empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Check_If_Org_Unit_Sales_Org_Matches</fullName>
        <active>true</active>
        <description>The Org Unit and the Sales Folder must have the same Sales Org.</description>
        <errorConditionFormula>ISPICKVAL(ACCL__Sales_Folder_Template__r.ACCL__Anchor_Type__c, &apos;OrgUnit&apos;) &amp;&amp; NOT(ISBLANK(ACCL__Org_Unit__c)) &amp;&amp; TEXT(ACCL__Org_Unit__r.ACCL__Sales_Org__c) &lt;&gt; TEXT(ACCL__Sales_Org__c)</errorConditionFormula>
        <errorMessage>The Org Unit and the Sales Folder must have the same Sales Org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Check_If_Sales_Org_Is_Changed</fullName>
        <active>true</active>
        <description>The Sales Org cannot be edited after Sales Folder creation.</description>
        <errorConditionFormula>ISCHANGED(ACCL__Sales_Org__c)</errorConditionFormula>
        <errorMessage>The Sales Org can&apos;t be edited after Sales Folder creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Check_If_Valid_From_More_Than_Valid_Thru</fullName>
        <active>true</active>
        <description>Valid From can&apos;t be later than Valid Thru.</description>
        <errorConditionFormula>ACCL__Valid_From__c  &gt;  ACCL__Valid_Thru__c</errorConditionFormula>
        <errorMessage>Valid From can&apos;t be later than Valid Thru.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Check_Sales_Folder_Template_Sales_Org</fullName>
        <active>true</active>
        <description>The Sales Folder and the Sales Folder Template must have the same Sales Org.</description>
        <errorConditionFormula>TEXT(ACCL__Sales_Org__c)!= TEXT(ACCL__Sales_Folder_Template__r.ACCL__Sales_Org__c)</errorConditionFormula>
        <errorMessage>The Sales Folder and the Sales Folder Template must have the same Sales Org.</errorMessage>
    </validationRules>

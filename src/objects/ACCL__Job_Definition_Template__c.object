    <validationRules>
        <fullName>ACCL__Avoid_POS_Surveys</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(ACCL__Measure_Type__c) == &apos;POS&apos; &amp;&amp; ACCL__Job_Template__r.ACCL__Product_Reference__c==true</errorConditionFormula>
        <errorMessage>POS Surveys can&apos;t be created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Check_If_Sales_Org_Is_Changed</fullName>
        <active>true</active>
        <description>The Sales Org can&apos;t be edited after Job Definition Template creation.</description>
        <errorConditionFormula>ISCHANGED(ACCL__Sales_Org__c)</errorConditionFormula>
        <errorMessage>The Sales Org can&apos;t be edited after Job Definition Template creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Consider_Max_Value</fullName>
        <active>true</active>
        <description>The Max Value can&apos;t be empty.</description>
        <errorConditionFormula>ACCL__Consider_Max_Value__c &amp;&amp;
      ISNULL(ACCL__Max_Value__c)</errorConditionFormula>
        <errorDisplayField>ACCL__Max_Value__c</errorDisplayField>
        <errorMessage>The Max Value can&apos;t be empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Consider_Min_Value</fullName>
        <active>true</active>
        <description>The Min Value can&apos;t be empty.</description>
        <errorConditionFormula>ACCL__Consider_Min_Value__c &amp;&amp; ISNULL(ACCL__Min_Value__c)</errorConditionFormula>
        <errorDisplayField>ACCL__Min_Value__c</errorDisplayField>
        <errorMessage>The Min Value can&apos;t be empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Consider_Value</fullName>
        <active>true</active>
        <description>If both Consider Min Value and Consider Max Value are used:
      MinValue &lt;= MaxValue</description>
        <errorConditionFormula>ACCL__Consider_Min_Value__c &amp;&amp;
      ACCL__Consider_Max_Value__c &amp;&amp;
      ACCL__Min_Value__c &gt; ACCL__Max_Value__c</errorConditionFormula>
        <errorMessage>The Min Value must be lower or equal than Max Value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Data_Type_Job_Template</fullName>
        <active>true</active>
        <description>The Data Type and the Job Template can&apos;t be edited after Job Definition Template creation.</description>
        <errorConditionFormula>ISCHANGED(ACCL__Data_Type__c) || ISCHANGED(ACCL__Job_Template__c)</errorConditionFormula>
        <errorMessage>The Data Type and the Job Template can&apos;t be edited after Job Definition Template creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Description</fullName>
        <active>false</active>
        <description>You must select at least one Lenguage Dependet Field.</description>
        <errorConditionFormula>ISBLANK(ACCL__Description_Language_1__c)
      &amp;&amp;
      ISBLANK(ACCL__Description_Language_2__c)
      &amp;&amp;
      ISBLANK(ACCL__Description_Language_3__c)</errorConditionFormula>
        <errorMessage>You must select at least one Lenguage Dependet Field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Picture_Taking_Not_Allowed</fullName>
        <active>true</active>
        <description>Picture Taking can only be selected if the related Job Template is &apos;Question&apos;.</description>
        <errorConditionFormula>ACCL__Picture_Taking__c  &amp;&amp;  ACCL__Job_Template__r.Name  &lt;&gt; &apos;Question&apos;</errorConditionFormula>
        <errorDisplayField>ACCL__Picture_Taking__c</errorDisplayField>
        <errorMessage>Picture Taking can only be selected if the related Job Template is &apos;Question&apos;</errorMessage>
    </validationRules>

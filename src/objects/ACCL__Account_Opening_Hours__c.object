    <validationRules>
        <fullName>ACCL__Time_Begin_End_Format_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
AND( 
NOT(ISBLANK(ACCL__Time_From__c)), 
NOT(REGEX(ACCL__Time_From__c, &quot;([01]?[0-9]|2[0-3]):[0-5][0-9]&quot;)) 
), 
AND( 
NOT(ISBLANK(ACCL__Time_Thru__c)), 
NOT(REGEX(ACCL__Time_Thru__c, &quot;([01]?[0-9]|2[0-3]):[0-5][0-9]&quot;)) 
) 
)</errorConditionFormula>
        <errorMessage>Please enter the correct format for Time From and Time Thru (hh:mm).</errorMessage>
    </validationRules>

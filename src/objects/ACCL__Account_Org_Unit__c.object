    <validationRules>
        <fullName>ACCL__Account_Not_Empty</fullName>
        <active>true</active>
        <errorConditionFormula>OR (ISBLANK(ACCL__Account__c), ISNULL(ACCL__Account__c))</errorConditionFormula>
        <errorDisplayField>ACCL__Account__c</errorDisplayField>

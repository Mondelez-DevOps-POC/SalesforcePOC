    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_Org_should_be_same</fullName>
        <active>true</active>
        <description>The Org Unit and the Customer Recipient must have the same Sales Org.</description>
        <errorConditionFormula>ACCL__Account__r.ACCL__Sales_Org__c &lt;&gt; TEXT( ACCL__Org_Unit__r.ACCL__Sales_Org__c )</errorConditionFormula>
        <errorMessage>The Org Unit and the Customer Recipient must have the same Sales Org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Valid_From_Valid_Thru_Validation</fullName>
        <active>true</active>
        <description>Valid From can&apos;t be later than Valid Thru.</description>
        <errorConditionFormula>ACCL__Valid_From__c &gt; ACCL__Valid_Thru__c</errorConditionFormula>

    <validationRules>
        <fullName>ACCL__Delivery_Recipient_Not_Empty</fullName>
        <active>true</active>
        <description>Validates if delivery recipient is empty.</description>
        <errorConditionFormula>AND(ISBLANK(ACCL__Delivery_Recipient__c),  ISPICKVAL(ACCL__Phase__c, &quot;Released&quot;))</errorConditionFormula>
        <errorMessage>No valid Delivery Recipients have been found for the Order Customer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Order_Template_Not_Empty</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( ACCL__Order_Template__c )</errorConditionFormula>
        <errorDisplayField>ACCL__Order_Template__c</errorDisplayField>
        <errorMessage>The Order Template can&apos;t be empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Wholesaler_Not_Empty</fullName>
        <active>true</active>
        <description>Validates if Wholesaler is empty when it is required in order template.</description>
        <errorConditionFormula>AND(ACCL__Order_Template__r.ACCL__Wholesaler_Required__c, ISBLANK(ACCL__Wholesaler__c), ISPICKVAL( ACCL__Phase__c , &quot;Released&quot;))</errorConditionFormula>
        <errorMessage>No valid Wholesalers have been found for the Order Customer.</errorMessage>
    </validationRules>

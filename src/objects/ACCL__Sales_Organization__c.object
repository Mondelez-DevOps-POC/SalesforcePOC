    <validationRules>
        <fullName>ACCL__Call_Start_Time_Format_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT(ISBLANK( ACCL__Call_Start_Time__c)), 
NOT(REGEX(ACCL__Call_Start_Time__c, &quot;([01]?[0-9]|2[0-3]):[0-5][0-9]&quot;)) 
)</errorConditionFormula>
        <errorDisplayField>ACCL__Call_Start_Time__c</errorDisplayField>
        <errorMessage>Please enter the correct format for Call Start Time (hh:mm).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Check_If_Name_Is_Unique</fullName>
        <active>true</active>
        <errorConditionFormula>and( 
Name = VLOOKUP 
($ObjectType.ACCL__Sales_Organization__c.Fields.Name , $ObjectType.ACCL__Sales_Organization__c.Fields.Name, Name ), 

not( 
Id=VLOOKUP 
($ObjectType.ACCL__Sales_Organization__c.Fields.Id , $ObjectType.ACCL__Sales_Organization__c.Fields.Name, Name )))</errorConditionFormula>
        <errorMessage>Sales Organization Name already exists, please enter different Name.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Enable_Daily_Accuracy_Mandatory</fullName>
        <active>true</active>
        <description>Enable Daily Accuracy should always be checked</description>
        <errorConditionFormula>NOT(ACCL__Enable_Daily_Accuracy__c)</errorConditionFormula>
        <errorDisplayField>ACCL__Enable_Daily_Accuracy__c</errorDisplayField>
        <errorMessage>Enable Daily Accuracy must be checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Language_Unique_Validation_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>IF(OR((NOT(ISBLANK(Text(ACCL__Language_1__c))) &amp;&amp; OR(Text(ACCL__Language_1__c)== Text(ACCL__Language_2__c), Text(ACCL__Language_1__c)== Text(ACCL__Language_3__c), Text(ACCL__Language_1__c)== Text(ACCL__Language_4__c))),(NOT(ISBLANK(TEXT(ACCL__Language_2__c))) &amp;&amp; OR(Text(ACCL__Language_2__c)== Text(ACCL__Language_3__c), Text(ACCL__Language_2__c)== Text(ACCL__Language_4__c))),(NOT(ISBLANK(Text(ACCL__Language_3__c))) &amp;&amp; (Text(ACCL__Language_3__c)== Text(ACCL__Language_4__c) ))),true,false)</errorConditionFormula>
        <errorMessage>Language already exists, please enter different Language.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_Org_Name_length</fullName>
        <active>true</active>
        <description>Name must be four characters.</description>
        <errorConditionFormula>LEN(Name) &lt;&gt; 4</errorConditionFormula>
        <errorMessage>Name must be four characters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Validate_Sales_Org_for_Week_Day_Profile</fullName>
        <active>true</active>
        <description>Sales Org of the Sales Organization record should match with the Sales Org of the selected Week Day Share Profile.</description>
        <errorConditionFormula>AND(
          NOT(ISBLANK( ACCL__Week_Day_Share_Profile__c)
          ),
          (
          TEXT(ACCL__Sales_Org_Value__c) &lt;&gt;
          TEXT(ACCL__Week_Day_Share_Profile__r.ACCL__Sales_Org__c)
          )
          )</errorConditionFormula>
        <errorMessage>The Sales Organization and the selected Week Day Share Profile must have the same Sales Org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Validation_rule_for_fund_template</fullName>
        <active>false</active>
        <errorConditionFormula>IF( ACCL__Enable_Auto_Tactic_Fund_Assignment__c == true  &amp;&amp;  ISBLANK(ACCL__Default_Tactic_Fund_Template__c),true,false  )</errorConditionFormula>
        <errorDisplayField>ACCL__Default_Tactic_Fund_Template__c</errorDisplayField>
        <errorMessage>Please select the Default Fund Template to proceed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Validation_rule_for_sales_org_name</fullName>
        <active>true</active>
        <description>Name must match with selected Sales Org value.</description>
        <errorConditionFormula>Name  &lt;&gt; Text( ACCL__Sales_Org_Value__c )</errorConditionFormula>
        <errorMessage>Name must match with selected Sales Org value.</errorMessage>
    </validationRules>

    <validationRules>
        <fullName>ACCL__Call_Template_Required</fullName>
        <active>true</active>
        <description>The Call Template can&apos;t be empty.</description>
        <errorConditionFormula>ISBLANK(ACCL__Call_Template__c)</errorConditionFormula>
        <errorMessage>The Call Template can&apos;t be empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Date_From_Has_To_Be_Before_Date_To</fullName>
        <active>true</active>
        <errorConditionFormula>ACCL__Start_Date_Time__c &gt; ACCL__End_Date_Time__c</errorConditionFormula>
        <errorMessage>Start Date Time can&apos;t be later than End Date Time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_Org_should_be_same</fullName>
        <active>true</active>
        <description>The Customer and the Tour must have the same Sales Org.</description>
        <errorConditionFormula>AND( ACCL__Tour__r.Id &lt;&gt; null,(TEXT( ACCL__Account__r.ACCL__Account_Template__r.ACCL__Sales_Org__c ) &lt;&gt; TEXT( ACCL__Tour__r.ACCL__Tour_Template__r.ACCL__Sales_Org__c )))</errorConditionFormula>
        <errorMessage>The Customer and the Tour must have the same Sales Org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_Org_should_be_same_for_all_lookups</fullName>
        <active>true</active>
        <description>The Customer and the Call must have the same Sales Org.</description>
        <errorConditionFormula>TEXT(ACCL__Call_Template__r.ACCL__Sales_Org__c) &lt;&gt; ACCL__Account__r.ACCL__Sales_Org__c</errorConditionFormula>
        <errorMessage>The Customer and the Call must have the same Sales Org.</errorMessage>
    </validationRules>

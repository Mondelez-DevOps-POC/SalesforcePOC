    <validationRules>
        <fullName>ACCL__Sales_Org_should_be_same</fullName>
        <active>true</active>
        <description>The Parent Org Unit and the Child Org Unit must have the same Sales Org.</description>
        <errorConditionFormula>TEXT(ACCL__Child_Org_Unit__r.ACCL__Sales_Org__c) &lt;&gt;  TEXT(ACCL__Parent_Org_Unit__r.ACCL__Sales_Org__c)</errorConditionFormula>
        <errorMessage>The Parent Org Unit and the Child Org Unit must have the same Sales Org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Same_Parent_Child_Org_Unit</fullName>
        <active>true</active>
        <description>The Child Org Unit and the Parent Org Unit can&apos;t be the same.</description>
        <errorConditionFormula>ACCL__Child_Org_Unit__c = ACCL__Parent_Org_Unit__c</errorConditionFormula>
        <errorMessage>The Child Org Unit and the Parent Org Unit can&apos;t be the same.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Valid_From_Valid_Thru_Validation</fullName>
        <active>true</active>
        <description>Valid From can&apos;t be later than Valid Thru.</description>
        <errorConditionFormula>ACCL__Valid_From__c &gt; ACCL__Valid_Thru__c</errorConditionFormula>
        <errorMessage>Valid From can&apos;t be later than Valid Thru.</errorMessage>
    </validationRules>

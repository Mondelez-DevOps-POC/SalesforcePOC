    <validationRules>
        <fullName>ACCL__Child_unequal_parent</fullName>
        <active>true</active>
        <description>The Child Product and the Parent Product can&apos;t be the same.</description>
        <errorConditionFormula>ACCL__Child_Product__c == ACCL__Parent_Product__c</errorConditionFormula>
        <errorMessage>The Child Product and the Parent Product can&apos;t be the same.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Mandatory_StructureType_is_set</fullName>
        <active>true</active>
        <description>The Structure Type can&apos;t be empty.</description>
        <errorConditionFormula>ISPICKVAL( ACCL__Structure_Type__c , &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>ACCL__Structure_Type__c</errorDisplayField>
        <errorMessage>The Structure Type can&apos;t be empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Parent_Product_Not_Empty</fullName>
        <active>true</active>
        <errorConditionFormula>OR (ISBLANK(ACCL__Parent_Product__c), ISNULL(ACCL__Parent_Product__c))</errorConditionFormula>
        <errorDisplayField>ACCL__Parent_Product__c</errorDisplayField>
        <errorMessage>The Parent Product can&apos;t be empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Product_Hierarchy_Sales_Org_Mismatch</fullName>
        <active>true</active>
        <description>The Child Product and the Parent Product must have the same Sales Org.</description>
        <errorConditionFormula>ACCL__Child_Product__r.ACCL__Sales_Org__c != ACCL__Parent_Product__r.ACCL__Sales_Org__c</errorConditionFormula>
        <errorMessage>The Child Product and the Parent Product must have the same Sales Org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Valid_From_and_Valid_Thru</fullName>
        <active>true</active>
        <description>Valid From can&apos;t be later than Valid Thru.</description>
        <errorConditionFormula>ACCL__Valid_From__c  &gt; ACCL__Valid_Thru__c</errorConditionFormula>
        <errorDisplayField>ACCL__Valid_Thru__c</errorDisplayField>
        <errorMessage>Valid From can&apos;t be later than Valid Thru.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>

    <validationRules>
        <fullName>ACCL__Sell_Sheet_Sales_Org_Equals_Sales_Folder</fullName>
        <active>true</active>
        <description>The Sales Folder Sell Sheet, Sales Folder and Sell Sheet must have the same Sales Org.</description>
        <errorConditionFormula>OR (TEXT(ACCL__Sell_Sheet__r.ACCL__Sales_Org__c) &lt;&gt; TEXT(ACCL__Sales_Org__c),
TEXT(ACCL__Sales_Folder__r.ACCL__Sales_Org__c) &lt;&gt; TEXT(ACCL__Sales_Org__c),
TEXT( ACCL__Sell_Sheet__r.ACCL__Sales_Org__c) &lt;&gt; TEXT(ACCL__Sales_Folder__r.ACCL__Sales_Org__c))</errorConditionFormula>
        <errorMessage>The Sales Folder Sell Sheet, Sales Folder and Sell Sheet must have the same Sales Org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_user_from_selecting_Other_KAcc</fullName>
        <active>true</active>
        <description>MDLZ_Restrict user from selecting Other KeyAccount</description>
        <errorConditionFormula>TEXT(ACCL__Sell_Sheet__r.MDLZ_KeyAccount__c) != ACCL__Sales_Folder__r.MDLZ_KeyAccount__c</errorConditionFormula>
        <errorMessage>Sales Folder and Sell Sheet attached should have same Key Account populated.</errorMessage>
    </validationRules>

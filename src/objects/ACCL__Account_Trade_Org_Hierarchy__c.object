    <validationRules>
        <fullName>ACCL__Child_Parent_Account_Same</fullName>
        <active>true</active>
        <description>The Child Customer and the Parent Customer can&apos;t be the same.</description>
        <errorConditionFormula>ACCL__Child_Account__c =  ACCL__Parent_Account__c</errorConditionFormula>
        <errorMessage>The Child Customer and the Parent Customer can&apos;t be the same.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Parent_Account_Not_Empty</fullName>
        <active>true</active>
        <errorConditionFormula>OR (ISBLANK(ACCL__Parent_Account__c), ISNULL(ACCL__Parent_Account__c))</errorConditionFormula>
        <errorDisplayField>ACCL__Parent_Account__c</errorDisplayField>
        <errorMessage>The Parent Customer can&apos;t be empty.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_Org_should_be_same</fullName>
        <active>true</active>
        <errorConditionFormula>ACCL__Child_Account__r.ACCL__Sales_Org__c &lt;&gt; ACCL__Parent_Account__r.ACCL__Sales_Org__c</errorConditionFormula>
        <errorMessage>The Child Customer and the Parent Customer must have the same Sales Org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Valid_From_Valid_Thru_Validation</fullName>
        <active>true</active>
        <description>Valid From can&apos;t be later than Valid Thru.</description>
        <errorConditionFormula>ACCL__Valid_From__c &gt; ACCL__Valid_Thru__c</errorConditionFormula>
        <errorMessage>Valid From can&apos;t be later than Valid Thru.</errorMessage>
    </validationRules>
